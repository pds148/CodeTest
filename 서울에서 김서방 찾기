class Solution {
    public String solution(String[] seoul) {
        String answer = "";
        
        int kimIndex = -1;
        for (int i=0; i<seoul.length; i++) {
            if (seoul[i].equals("Kim")) {
                kimIndex = i;
                break;
            }
        }
        
        answer = "김서방은 " + kimIndex + "에 있다";
        return answer;
    }
}
